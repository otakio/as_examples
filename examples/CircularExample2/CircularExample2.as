package  {	import flash.display.MovieClip;	import flash.events.Event;	import flash.geom.Point;	public class CircularExample2 extends MovieClip{				//中心座標		var center:Point;		//親の回転角度		var angle:Number = 0.0;		//子の回転角度		var angle2:Number = 0.0;		//親の回転半径		var r:Number = 150;		//子の回転半径		var r2:Number = 50;		//親		var symbol:MySymbol;		//子		var symbol2:MySymbol2;		//中心		var centerSymbol:CenterSymbol;		public function CircularExample2() {			// constructor code			center = new Point(300, 225);			//親の初期化と配置			symbol = new MySymbol();			symbol.x = 150;			symbol.y = 225;			this.addChild(symbol);						//子の初期化と配置			symbol2 = new MySymbol2();			symbol2.x = 140;			symbol2.y = 225;			this.addChild(symbol2);						//中心の初期化と配置			centerSymbol = new CenterSymbol();			centerSymbol.x = center.x;			centerSymbol.y = center.y;			this.addChild(centerSymbol);						this.addEventListener(Event.ENTER_FRAME, loop);		}		function loop(e:Event):void{			//親の座標計算			angle += 0.02;			symbol.x = center.x + r * Math.cos(angle);			symbol.y = center.y + r * Math.sin(angle);			//子の座標計算			angle2 += 0.04;			symbol2.x = symbol.x + r2 * Math.cos(angle2);			symbol2.y = symbol.y + r2 * Math.sin(angle2);		}	}	}
package  {	import flash.display.MovieClip;	import flash.events.Event;		public class StageInstanceExample extends MovieClip{		public function StageInstanceExample() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);		}				function loop(e:Event):void{			this["yamada"].x += 1;			this["suzuki"].x -= 1;			this["sakamoto"].y += 1;			this["kohno"].y -= 1;			this["honda"].rotation += 1;			this["ota"].rotation -=1;			this["tanaka"].scaleX += 0.01;			this["tanida"].scaleY += 0.01;		}			}	}
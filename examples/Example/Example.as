package  {	//MovieClipクラスとEventクラスをインポート	import flash.display.MovieClip;	import flash.events.Event;		//MovieClipクラスの継承	public class Example extends MovieClip{				//MySymbolクラスを継承したインスタンス「mySymbol」の宣言。インスタンス名はなんでもよいが1文字目は小文字に。		var mySymbol:MySymbol;		public function Example() {			// constructor code			//ENTER_FRAMEイベントの際にloopメソッドを呼ぶ			this.addEventListener(Event.ENTER_FRAME, loop);						mySymbol = new MySymbol();	//インスタンス「mySymbol」の初期化			mySymbol.x = 300;	//xプロパティを300に			mySymbol.y = 225;	//yプロパティを225に			this.addChild(mySymbol);	//インスタンスステージ上に配置		}		//メインループ		function loop(e:Event):void{			//mySymbolのrotationプロパティに2ずつ加える（2度ずつ回転）			mySymbol.rotation = mySymbol.rotation + 2;		}	}	}
package  {	/* MovieClip, Eventをインポート */	import flash.display.MovieClip;	import flash.events.Event;		/* クラス宣言にMovieClip継承を追加 */	public class StartExample extends MovieClip{		/* ここにクラスが持つメンバ変数を書く。今は空欄でよい。 */		public function StartExample() {			// constructor code						/* クラス名のメソッドはコンストラクタと呼ばれ、ここに最初にしたいことを書く。 */						/* ENTER_FRAME時に呼ばれるメソッドをEventListenerに登録。 */			this.addEventListener(Event.ENTER_FRAME, loop);		}				/* ENTER_FRAME時に呼ばれるメソッド「loop」 */		function loop(e:Event):void{			trace("Hello World");		}	}	}
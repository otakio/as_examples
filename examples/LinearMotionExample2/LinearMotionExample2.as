package  {	import flash.display.MovieClip;	import flash.events.Event;		public class LinearMotionExample2 extends MovieClip{		var oval:Oval;		//x方向、y方向への速度		var addX:Number;		var addY:Number;		public function LinearMotionExample2() {			// constructor code			oval = new Oval();			oval.x = 0;			oval.y = 0;			this.addChild(oval);						addX = 2;	//addXに2を代入			addY = 1.5;	//addYに1.5を代入						this.addEventListener(Event.ENTER_FRAME, loop);		}		function loop(e:Event):void{			//ovalのxにaddXずつ加える			oval.x = oval.x + addX;			//ovalのyにaddYずつ加える			oval.y = oval.y + addY;		}	}	}
package  {	import flash.display.MovieClip;	import flash.events.Event;	import flash.geom.Point;	import flash.utils.Timer;	import flash.events.TimerEvent;		public class Scene3 extends MovieClip{				var oval:OvalSymbol;		var pts:Array;		var curPt:int;				var timer:Timer;		public function Scene3() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);						pts = new Array();			pts.push(new Point(150, 75));			pts.push(new Point(150, 375));			pts.push(new Point(450, 375));			pts.push(new Point(450, 75));						curPt = 0;			oval = new OvalSymbol();			oval.x = pts[curPt].x;			oval.y = pts[curPt].y;			this.addChild(oval);								timer = new Timer(666);			timer.addEventListener(TimerEvent.TIMER, onTimerEvent);			timer.start();		}		function loop(e:Event):void{			oval.x += (pts[curPt].x - oval.x)/10.0;			oval.y += (pts[curPt].y - oval.y)/10.0;		}				function onTimerEvent(e:TimerEvent):void{			curPt += 1;			if(curPt > pts.length-1){				curPt = 0;			}		}	}	}
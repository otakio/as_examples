package  {	import flash.display.MovieClip;	import flash.events.Event;	import flash.geom.Point;		public class Scene1 extends MovieClip{				var oval:OvalSymbol;		var radius:Number;		var center:Point;		var angle:Number;		public function Scene1() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);						radius = 180;			center = new Point(300, 225);			angle = 0;						oval = new OvalSymbol();			oval.x = center.x + radius * Math.cos(angle);			oval.y = center.y + radius * Math.sin(angle);			this.addChild(oval);								}		function loop(e:Event):void{			angle += 0.05;			oval.x = center.x + radius * Math.cos(angle);			oval.y = center.y + radius * Math.sin(angle);		}	}	}
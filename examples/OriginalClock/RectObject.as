package  {	import flash.display.Sprite;	import flash.events.Event;	import FlaUtils.FlaGraphics;		public class RectObject extends Sprite{		public var isOn:Boolean = false;		public function RectObject(rr:int=0, gg:int=0, bb:int=0) {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);			FlaGraphics.setColor(rr, gg, bb);			FlaGraphics.fillRect(this, -29, -29, 58, 58);			this.scaleX = 0;			this.scaleY = 0;		}		function loop(e:Event):void{			if(isOn){				this.scaleX += (1.0 - this.scaleX) / 10.0;				this.scaleY += (1.0 - this.scaleY) / 10.0;			}else{				this.scaleX += (0 - this.scaleX) / 10.0;				this.scaleY += (0 - this.scaleY) / 10.0;							}		}	}	}
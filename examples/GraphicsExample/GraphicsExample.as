package  {	import flash.display.MovieClip;	import flash.events.Event;	import FlaUtils.FlaGraphics;		public class GraphicsExample extends MovieClip{			var radius:Number = 25;			var counter:Number = 0.01;		public function GraphicsExample() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);					}		function loop(e:Event):void{			this.graphics.clear();			//色を設定			FlaGraphics.setColor(0, 0, 0);			//線の太さを設定			FlaGraphics.setLineWidth(1);			//線を描画			FlaGraphics.drawLine(this, 50, 100, 300, 400);			//スプライン曲線を描画			FlaGraphics.drawSprineCurve(this, 50, 100, 200, 200, 300, 100);			//ベジェ曲線を描画（アンカーポイント1の(x,y)、制御点1の(x,y)、制御点2の(x,y)、アンカーポイント2の(x,y)）			FlaGraphics.drawBezierCurve(this, 50, 300, 50, 0, 300, 600, 300, 300);			//色を設定			FlaGraphics.setColor(255, 0, 0);			//円（線）を描画			FlaGraphics.drawCircle(this, 375, 100, 50)			//円（塗り）を描画			FlaGraphics.fillCircle(this, 375, 100, 25);			//色を再設定			FlaGraphics.setColor(0, 200, 255);			//半径に変数を噛ませる			FlaGraphics.fillCircle(this, 500, 100, 25+radius * Math.sin(counter));						//色を再設定			FlaGraphics.setColor(0, 255, 0);			//長方形（線）を描画			FlaGraphics.drawRect(this, 350, 200, 50, 100);			//長方形（塗り）を描画			FlaGraphics.fillRect(this, 450, 200, 100, 50);			//アルファつき色設定			FlaGraphics.setColor(0, 0, 255, 100);			FlaGraphics.fillRect(this, 450, 200, 50, 100);			//長方形の座標に変数を噛ませる			FlaGraphics.setColor(255, 0, 0, 100);			FlaGraphics.fillRect(this, 450 + 50 * Math.sin(counter), 275, 100, 25);			//長方形のアルファと座標に変数を噛ませる			FlaGraphics.setColor(255, 255, 0, 150 - 100 * Math.cos(counter));			FlaGraphics.fillRect(this, 525, 150 + 100 * Math.sin(counter), 25, 100);						counter += 0.01;		}	}	}
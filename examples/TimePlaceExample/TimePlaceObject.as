package  {	import flash.display.MovieClip;	import flash.geom.Point;	import flash.utils.Timer;	import flash.events.TimerEvent;    import flash.text.*;		public class TimePlaceObject extends MovieClip{				public var time:Number;		var oPt:Point;		var iPt:Point;		var timer:Timer;		var label:TextField;		public function TimePlaceObject(ox:int, oy:int, theTime:Number) {			// constructor code			x = ox;			y = oy;			oPt = new Point(ox, oy);			iPt = new Point(ox, oy);			time = theTime;			timer = new Timer(33);			timer.addEventListener(TimerEvent.TIMER, loop);			timer.start();			label = new TextField();            label.text      = time.toString();            label.autoSize  = TextFieldAutoSize.LEFT;            label.selectable = false;                 			this.addChild(label);		}		function loop(e:TimerEvent):void{			var addX:Number = (iPt.x - x) * 0.1;			var addY:Number = (iPt.y - y) * 0.1;			x += addX;			y += addY;			if(Math.abs(x - iPt.x) < 1.0)				x = iPt.x;			if(Math.abs(y - iPt.y) < 1.0)				y = iPt.y;			trace(x + "," + y);		}	}	}
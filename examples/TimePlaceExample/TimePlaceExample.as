package  {	import flash.display.MovieClip;	import flash.events.Event;	import flash.geom.Point;	import flash.events.MouseEvent;	public class TimePlaceExample extends MovieClip{		var objects:Array;		var isTime:Boolean;		const monitorWidth:uint = 600;		const monitorHeight:uint = 450;		public function TimePlaceExample() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);			objects = new Array();			for(var i:uint = 0; i < 10; i ++){				objects.push(new TimePlaceObject(50 + Math.random()*(monitorWidth-100), 												 50+Math.random()*(monitorHeight-100), 												 (int)(1000 * Math.random())												 ));				this.addChild(objects[i]);							}			isTime = false;			this.stage.addEventListener(MouseEvent.MOUSE_UP, onClicked);		}		function loop(e:Event):void {		}		function onClicked(e:MouseEvent):void{			trace("aaa");			if(!isTime){				objects.sortOn("time", Array.DESCENDING | Array.NUMERIC);				var curX:int = 0;				for(var i:uint = 0; i < objects.length; i ++){					objects[i].iPt.x = curX;					objects[i].iPt.y = this.stage.stageHeight/2;					curX += objects[i].width;				}				isTime = true;			}else{				for(i = 0; i < objects.length; i ++){					objects[i].iPt.x = objects[i].oPt.x;					objects[i].iPt.y = objects[i].oPt.y;					curX += 100;				}				isTime = false;			}		}	}	}
package  {	import flash.display.MovieClip;	import flash.events.Event;	import FlaUtils.*;		public class Example extends MovieClip{				var ovals:Array;		public function Example() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);						ovals = new Array();			var curX:int = 50;			var curY:int = 50;			for(var i:uint = 0; i < 100; i++){				ovals.push(new Oval());				ovals[i].setPoint(curX, curY);				curX += 30;				if(i%10 == 9){					curY += 30;					curX = 50;				}				this.addChild(ovals[i]);			}		}		function loop(e:Event):void{		}	}	}
package  {	import flash.display.MovieClip;	import flash.events.Event;		public class JojoniMouseExample extends MovieClip{				var mySymbol:MySymbol;		public function JojoniMouseExample() {			// constructor code			mySymbol = new MySymbol();			mySymbol.x = 300;			mySymbol.y = 225;			this.addChild(mySymbol);						this.addEventListener(Event.ENTER_FRAME, loop);		}		function loop(e:Event):void{			mySymbol.x += (mouseX - mySymbol.x) / 10.0;			mySymbol.y += (mouseY - mySymbol.y) / 10.0;		}	}	}
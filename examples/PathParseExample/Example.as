package  {	import flash.display.MovieClip;	import flash.events.Event;	import flash.net.URLRequest;	import flash.net.URLLoader;	import FlaUtils.*;	import com.zavoo.*;	import com.zavoo.svg.SvgPaths;		public class Example extends MovieClip{				var strokes:SvgPaths;		public function Example() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, loop);						var loader:URLLoader = new URLLoader();			loader.addEventListener(Event.COMPLETE, onSVGLoaded);			loader.load(new URLRequest("6_8.svg"));		}				function onSVGLoaded(e:Event):void{			try{				strokes = new SvgPaths(e.target.data);				strokes.drawToGraphics(this.graphics, 1.0, 0, 0);				}catch(err:TypeError){				trace(err);			}					}		function loop(e:Event):void{//			this.graphics.clear();//			strokes.drawToGraphics(this.graphics, 1.0, 0, 0);		}	}	}
package  {	import flash.display.MovieClip;	import flash.events.Event;		public class LinearMotionExample extends MovieClip{		var oval:Oval;		public function LinearMotionExample() {			// constructor code			oval = new Oval();			oval.x = 0;			oval.y = 200;			this.addChild(oval);						this.addEventListener(Event.ENTER_FRAME, loop);		}		function loop(e:Event):void{			//ovalのxに1ずつ加える			oval.x = oval.x + 1;		}	}	}
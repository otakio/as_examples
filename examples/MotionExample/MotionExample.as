package  {	import flash.display.MovieClip;	import flash.events.Event;	import flash.events.MouseEvent;		public class MotionExample extends MovieClip{				var oval:Oval;				public function MotionExample() {			// constructor code						oval = new Oval();			oval.x = 0;			oval.y = 100;			this.addChild(oval);			this.addEventListener(Event.ENTER_FRAME, loop);					}		function loop(e:Event):void{			oval.x = oval.x + (600 - oval.x) / 10.0;			oval.y = oval.y + (300 - oval.y) / 10.0;		}			}	}
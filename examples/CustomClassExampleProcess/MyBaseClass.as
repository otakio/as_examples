package  {	import flash.display.MovieClip;	import flash.geom.Point;		public class MyBaseClass extends MovieClip{				public var birthYear:Number;	//生年		public var shincho:Number;	//身長		public var jimotoPt:Point;	//地元の座標		public var ix:Number;	//行き先のx座標（アニメーションさせる際の目的地）		public var iy:Number;	//行き先のy座標（アニメーションさせる際の目的地）		public function MyBaseClass() {			// 各プロパティに初期値を入れる			birthYear = 0;			shincho = 0;			jimotoPt = new Point(0, 0);			ix = 0;			iy = 0;		}	}	}
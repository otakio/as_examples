package  {	import flash.display.MovieClip;	import flash.events.Event;		public class JojoniArrayExample extends MovieClip{				//配列の宣言（Arrayインスタンス）		var symbols:Array;		//アニメーション用の変化率		var duration:Number = 10.0;		public function JojoniArrayExample() {			// constructor code			//配列を初期化			symbols = new Array();			//100回ループして要素を挿入する			for(var i:uint = 0; i < 100; i++){				symbols.push(new MySymbol());				symbols[i].x = 300;				symbols[i].y = 225;				this.addChild(symbols[i]);	//ステージに配置			}						this.addEventListener(Event.ENTER_FRAME, loop);		}		function loop(e:Event):void{			//0個目の要素だけマウスについていく			symbols[0].x += (mouseX - symbols[0].x) / duration;			symbols[0].y += (mouseY - symbols[0].y) / duration;						//残りは1個前の要素についていく。			for(var i:uint = 1; i < symbols.length; i++){				symbols[i].x += (symbols[i-1].x - symbols[i].x) / duration;				symbols[i].y += (symbols[i-1].y - symbols[i].y) / duration;			}		}	}	}
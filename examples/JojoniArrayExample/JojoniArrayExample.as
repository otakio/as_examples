package  {	import flash.display.MovieClip;	import flash.events.Event;		public class JojoniArrayExample extends MovieClip{				var symbols:Array;		var duration:Number = 10.0;		public function JojoniArrayExample() {			// constructor code			symbols = new Array();						for(var i:uint = 0; i < 30; i++){				symbols[i] = new MySymbol();				symbols[i].x = 300;				symbols[i].y = 225;				this.addChild(symbols[i]);			}						this.addEventListener(Event.ENTER_FRAME, loop);		}		function loop(e:Event):void{			symbols[0].x += (mouseX - symbols[0].x) / duration;			symbols[0].y += (mouseY - symbols[0].y) / duration;						for(var i:uint = 1; i < symbols.length; i++){				symbols[i].x += (symbols[i-1].x - symbols[i].x) / duration;				symbols[i].y += (symbols[i-1].y - symbols[i].y) / duration;			}		}	}	}
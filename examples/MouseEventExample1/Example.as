package  {    import flash.display.MovieClip;    import flash.events.Event;    import flash.events.MouseEvent;         public class Example extends MovieClip{                 var mySymbol:MySymbol;        var honda:Honda;        var notta:Boolean;         public function Example() {            // constructor code                                     honda = new Honda();            honda.x =300;            honda.y =225;            this.addChild(honda);			mySymbol = new MySymbol();            mySymbol.x = 0;            mySymbol.y = 225;            this.addChild(mySymbol);						mySymbol.mouseEnabled = false;                         honda.addEventListener(MouseEvent.ROLL_OVER, onROver);            honda.addEventListener(MouseEvent.ROLL_OUT, onROut);                         notta = false;                         this.addEventListener(Event.ENTER_FRAME, loop);        }        function loop(e:Event):void{//          trace(notta);            if(notta == true){                mySymbol.x += (mouseX - mySymbol.x) / 30.0;                mySymbol.y += (mouseY - mySymbol.y) / 30.0;            }else{                mySymbol.x += (0 - mySymbol.x) / 30.0;                mySymbol.y += (225 - mySymbol.y) / 30.0;                             }        }                 function onROver(e:MouseEvent):void{            trace("Roll Over!!");            notta = true;        }        function onROut(e:MouseEvent):void{            trace("Roll Out!!");            notta = false;        }     }     }
package  {	import flash.display.MovieClip;		public class FukidashiExample extends MovieClip{				var objects:Array;		var monitorWidth:int = 600;		var monitorHeight:int = 450;		public function FukidashiExample() {			// constructor code						var curX:int = 50;			var curY:int = 120;			objects = new Array();			for(var i:uint = 0; i < 33; i++){				objects.push(new Person());				objects[i].x = curX;				objects[i].y = curY;				curX += 50;				if(curX > monitorWidth - 50){					curX = 50;					curY += 100;				}				this.addChild(objects[i]);			}			var atariNum:uint = Math.random()*objects.length;			trace(atariNum);			objects[atariNum].fukidashi.textField.text = "I'm Honda";		}			}	}
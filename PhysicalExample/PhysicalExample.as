package  {	import flash.display.MovieClip;	import flash.events.Event;	import FlaUtils.*;		public class PhysicalExample extends MovieClip{		var pt:PhysicalPoint;		var mySymbol:MySymbol;		public function PhysicalExample() {			// constructor code			mySymbol = new MySymbol();			mySymbol.x = 300;			mySymbol.y = 300;			this.addChild(mySymbol);						pt = new PhysicalPoint(mySymbol.x, mySymbol.y);			this.addEventListener(Event.ENTER_FRAME, loop);		}				function loop(e:Event):void{			mySymbol.x = pt.x;			mySymbol.y = pt.y;			pt.setAccelerationTo(mouseX, mouseY, 10);					}	}	}